import random
import time
import pyttsx3

def speak(text):
     engine = pyttsx3.init()
     engine.say(text)
     engine.runAndWait()

list = [
    "All the way up",
    "All the way down",
    "Half way down",
    "One quarter down",
    "Three quarters down"
]

print("Welcome to the jungle!")
speak("Welcome to the jungle!")
time.sleep(3.5)

while True:
    value = random.randint(0, 4)
    print(list[value])
    speak(list[value])
    time.sleep(random.randint(5, 10))